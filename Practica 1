<h1>PROGRAMA-CALCULAR LOS PUNTOS DE UN EQUIPO DE FUTBOL</h1>
<script>
	function saltarLinea(){
	document.write("<br>");
	document.write("<br>");
	}
	function imprimir(frase){
		document.write(frase);
		saltarLinea();
	}
	var victoria=parseInt(prompt("Ingrese la cantidad de victorias"));
	var empate=parseInt(prompt("Ingrese la cantida de empates"));
	puntosTotales=(victoria*3)+empate;
	imprimir("La cantidad de puntos del equipo es: "+puntosTotales);

</script>
